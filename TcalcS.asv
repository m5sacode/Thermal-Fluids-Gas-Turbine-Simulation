%% calculate T given s bar
% works for temperatures between 300K, 3000K

function T = TcalcS(sb)

err = 1;
Tmin = 250;
Tmax = 1800;

while err > 1e-3
    T = avg(Tmin, Tmax);
    scalc = valInterp(T, 1);
    err = abs(hb-hcalc);
    if sb > hcalc
        Tmin = T;
    elseif hb < hcalc
        Tmax = T;
    end    
end

end